033D:                   M8_C_tend_c46_handler:
033D: D9       [ 4]     		exx 								; use alt registers
033E: E1       [14]     		pop 	hl 							; return/offset address.
033F: D1       [24]     		pop 	de 							; this is the count
0340: 7A       [28]     		ld 		a,d 						; check if it is zero.
0341: B3       [32]     		or 		e
0342: 2004     [39|44]  		jr 		nz,_tend_loop 				; if non zero, loop back
                        		;
0344: 23       [45]     		inc 	hl 							; exit loop, skip the count back.
0345: E5       [56]     		push 	hl 							; push return address back on stack
0346: D9       [60]     		exx 								; get original registers back and exit
0347: C9       [70]     		ret
                        
0348:                   _tend_loop:									; we are going round the loop, return HL count DE
0348: 4E       [ 7]     		ld 		c,(hl) 						; get the loop offset into BC
0349: 0600     [14]     		ld 		b,0
034B: AF       [18]     		xor 	a 							; subtract from HL
034C: ED42     [33]     		sbc 	hl,bc
034E: E5       [44]     		push 	hl 	 						; push loop address on stack
034F: 1B       [50]     		dec 	de 							; decrement counter
0350: D5       [61]     		push 	de  						; push new count on stack
0351: D9       [65]     		exx 								; original registers
0352: E1       [75]     		pop 	hl 							; count into HL
0353: C9       [85]     		ret 								; and exit
                        
0354:                   M8_C_tend_c46_handler_end:
