                        ; *********************************************************************************************
                        ; *********************************************************************************************
                        ;
                        ;		Name:		spritemanager.asm
                        ;		Purpose:	Sprite Manager
                        ;		Created:	1st November 2021
                        ;		Author:		Paul Robson (paul@robsons.org.uk)
                        ;
                        ; *********************************************************************************************
                        ; *********************************************************************************************
                        ;
                        ;		Sprite Record:
                        ;
                        ; 			Current Data: (as per xsprite.asm)
                        ;				0..6 	X:2 	Y:2 	Graphics:2 	Control:1
                        ;				7 		Status byte
                        ;			To Copy data:
                        ;				8..14 	X:2 	Y:2 	Graphics:2 	Control:1
                        ;				15 		Change flag
                        ;
                        ;		When being updated, if the change flag is set, then the sprite is removed, then data
                        ;		(8-14) is copied to (0-6), then the sprite redrawn
                        ;
                        ;		The option also exists to erase all sprites ; the point of such being that one can update
                        ;		the background. This sets all the change flags so the sprites are redrawn on the next sync.
                        ;
                        ; *********************************************************************************************
                        ;
                        ;
                        ;			Reset Sprite Manager. HL points to data block, DE is number of sprites
                        ;
                        ;
                        ; *********************************************************************************************
                        
03EA:                   M8_C_spr_c46_reset:
                        
03EA:                   SPMReset:
03EA: F5       [11]     		push 	af
03EB: C5       [22]     		push 	bc
03EC: D5       [33]     		push 	de
03ED: E5       [44]     		push 	hl
03EE: 22F204   [60]     		ld 		(SPMData),hl 				; save count and address
03F1: 7B       [64]     		ld 		a,e
03F2: 32F404   [77]     		ld 		(SPMCount),a
03F5: 87       [81]     		add 	a,a 						; double, as we clear it twice (2 x 8 byte blocks)
03F6: 47       [85]     		ld 		b,a
03F7:                   _SPMClear:
03F7: 0E06     [ 7]     		ld 		c,6 						; write out 6 $FFs to position and graphic
03F9:                   _SPMClear2:
03F9: 36FF     [10]     		ld 		(hl),$FF
03FB: 23       [16]     		inc 	hl
03FC: 0D       [20]     		dec 	c
03FD: 20FA     [27|32]  		jr 		nz,_SPMClear2
03FF: 71       [34]     		ld 		(hl),c 						; followed by 2 zeros (control and status/change byte)
0400: 23       [40]     		inc 	hl
0401: 71       [47]     		ld 		(hl),c
0402: 23       [53]     		inc 	hl
0403: 10F2     [61|66]  		djnz 	_SPMClear
0405: 21F804   [71]     		ld 		hl,SPMUnused 				; no current selection
0408: 22F604   [87]     		ld 		(SPMCurrent),hl
040B: CD0805   [104]    		call 	SPRInitialise 				; erase the sprite control records.
040E: E1       [114]    		pop 	hl
040F: D1       [124]    		pop 	de
0410: C1       [134]    		pop 	bc
0411: F1       [144]    		pop 	af
0412: C9       [154]    		ret
                        
